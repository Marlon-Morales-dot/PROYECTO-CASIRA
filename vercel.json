{
  "functions": {
    "frontend/dist/**": {
      "runtime": "@vercel/static"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/$1"
    },
    {
      "src": "/[^.]+",
      "dest": "/frontend/dist/index.html"
    }
  ],
  "buildCommand": "cd frontend && npm ci && npm run build"
}
